{
	"id": "nodebb-plugin-customize-groups",
	"url": "https://github.com/robyweb/nodebb-plugin-customize-groups",
	"library": "./library.js",
	"hooks": [
	  	{
			"hook": "action:group.update", "method": "groupUpdate"
		},
	  	{
			"hook": "filter:group.create", "method": "groupCreate"
		}
,
	  	{
			"hook": "filter:groups.get", "method": "groupGet"
		}

	],
	"staticDirs": {
	  "public": "public"
	},
  "scripts": ["static/showOwner.js"],
  "acpScripts": [
	"public/src/admin/manage/group.js"
  ],
	"templates": "static/templates",
	"languages": "public/language",
	"defaultLang": "en-US"
}